<!DOCTYPE html>
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Mi aplicación ASP.NET</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div class="super_container">
        <header class="header">
            <div class="container">

                <div class="row">
                    <div class="col">
                        <div class="header_container d-flex flex-row align-items-center justify-content-start">

                            <!-- Logo -->
                            <div class="logo_container">
                                <div class="logo">
                                    <div>UTP</div>
                                    <div>Travel agency</div>
                                    <div class="logo_image"><img src="~/Content/assets/images/logo.png" alt=""></div>
                                </div>
                            </div>

                            <!-- Main Navigation -->
                            <nav class="main_nav ml-auto">
                                <ul class="main_nav_list">
                                    <li class="main_nav_item active">@Html.ActionLink("Inicio","Index","Home")</li>
                                    <li class="main_nav_item">@Html.ActionLink("informacion de tu viaje", "InfoViaje", "Home")</li>
                                    <li class="main_nav_item">@Html.ActionLink("administra de tu viaje", "AdminViaje", "Home")</li>
                                    <li class="main_nav_item">@Html.ActionLink("imprime el pasaje", "ImprimePasaje", "Home")</li>
                                </ul>
                            </nav>

                            <!-- Search -->
                            <div class="search">
                                <form action="#" class="search_form">
                                    <button type="submit" class="search_button ml-auto ctrl_class"><i class="fa fa-user" data-toggle="modal" data-target="#exampleModal"></i></button>
                                </form>
                            </div>

                            <!-- Hamburger -->
                            <div class="hamburger ml-auto"><i class="fa fa-bars" aria-hidden="true"></i></div>

                        </div>
                    </div>
                </div>
            </div>
        </header>
        <div class="menu_container menu_mm">

            <!-- Menu Close Button -->
            <div class="menu_close_container">
                <div class="menu_close"></div>
            </div>

            <!-- Menu Items -->
            <div class="menu_inner menu_mm">
                <div class="menu menu_mm">
                    <div class="menu_search_form_container">
                        <form action="#" id="menu_search_form">
                            <input type="search" class="menu_search_input menu_mm">
                            <button id="menu_search_submit" class="menu_search_submit" type="submit"><img src="~/Content/assets/images/search_2.png" alt=""></button>
                        </form>
                    </div>
                    <ul class="menu_list menu_mm">
                        <li class="menu_item menu_mm"><a href="#">Home</a></li>
                        <li class="menu_item menu_mm"><a href="about.html">About us</a></li>
                        <li class="menu_item menu_mm"><a href="offers.html">Offers</a></li>
                        <li class="menu_item menu_mm"><a href="news.html">News</a></li>
                        <li class="menu_item menu_mm"><a href="contact.html">Contact</a></li>
                    </ul>

                    <!-- Menu Social -->

                    <div class="menu_social_container menu_mm">
                        <ul class="menu_social menu_mm">
                            <li class="menu_social_item menu_mm"><a href="#"><i class="fa fa-pinterest" aria-hidden="true"></i></a></li>
                            <li class="menu_social_item menu_mm"><a href="#"><i class="fa fa-linkedin" aria-hidden="true"></i></a></li>
                            <li class="menu_social_item menu_mm"><a href="#"><i class="fa fa-instagram" aria-hidden="true"></i></a></li>
                            <li class="menu_social_item menu_mm"><a href="#"><i class="fa fa-facebook" aria-hidden="true"></i></a></li>
                            <li class="menu_social_item menu_mm"><a href="#"><i class="fa fa-twitter" aria-hidden="true"></i></a></li>
                        </ul>
                    </div>

                    <div class="menu_copyright menu_mm">Colorlib All rights reserved</div>
                </div>

            </div>

        </div>
        <div class="home" style="height:366px">
            <div class="home_background" style="background-image:url(/Content/assets/images/home.jpg)"></div>
            <div class="home_content">
                <div class="home_content_inner">
                    <div class="home_text_large">discover</div>
                    <div class="home_text_small">Descrube nuevos Lugares</div>
                </div>

            </div>
        </div>
        @RenderBody()

        <footer class="footer">
            <div class="container">
                <div class="row">

                    <!-- Footer Column -->
                    <div class="col-lg-4 footer_col">
                        <div class="footer_about">
                            <!-- Logo -->
                            <div class="logo_container">
                                <div class="logo">
                                    <div>UTP</div>
                                    <div>TRAVEL AGENCY</div>
                                    <div class="logo_image"><img src="~/Content/assets/images/logo.png" alt=""></div>
                                </div>
                            </div>
                            <div class="footer_about_text">Las compras en UTP travel angency no tienen un costo adicional sobre el precio de los pasajes (vendemos al mismo precio que en el counter/terminal)</div>
                            <div class="copyright">
                                Copyright &copy;
                                <script>document.write(new Date().getFullYear());</script> All rights reserved | Esta pagina es propiedad de Julia <i class="fa fa-heart-o" aria-hidden="true"></i> by <a href="https://colorlib.com" target="_blank">Colorlib</a>
                            </div>
                        </div>
                    </div>

                    <!-- Footer Column -->
                    <div class="col-lg-4 footer_col">
                        <div class="footer_latest">
                            <div class="footer_title">lugares mas visitados</div>
                            <div class="footer_latest_content">

                                <!-- Footer Latest Post -->
                                <div class="footer_latest_item">
                                    <div class="footer_latest_image"><img src="~/Content/assets/images/latest_1.jpg"></div>
                                    <div class="footer_latest_item_content">
                                        <div class="footer_latest_item_title">@Html.ActionLink("Machu picchu", "Index", "Home")</div>
                                        <div class="footer_latest_item_date">¡Lo mas bello!</div>
                                    </div>
                                </div>

                                <!-- Footer Latest Post -->
                                <div class="footer_latest_item">
                                    <div class="footer_latest_image"><img src="~/Content/assets/images/latest_2.jpg"></div>
                                    <div class="footer_latest_item_content">
                                        <div class="footer_latest_item_title"><a href="news.html">Puno-Uros</a></div>
                                        <div class="footer_latest_item_date">¡Inolvidable!</div>
                                    </div>
                                </div>

                            </div>
                        </div>
                    </div>

                    <!-- Footer Column -->
                    <div class="col-lg-4 footer_col">
                        <div class="tags footer_tags">
                            <div class="footer_title">Tags</div>
                            <ul class="tags_content d-flex flex-row flex-wrap align-items-start justify-content-start">
                                <li class="tag"><a href="#">Mejores Buses</a></li>
                                <li class="tag"><a href="#">Pasajes Baratos</a></li>
                                <li class="tag"><a href="#">Lugares recomendados</a></li>
                            </ul>
                        </div>
                    </div>

                </div>
            </div>
            <button type="button" class="btn btn-danger" data-toggle="modal" data-target="#administrador">Administrador</button>
        </footer>

    </div>


    <div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">Login</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">


                    <div class="container">
                        <div class="row">
                            <div class="col-md-12">
                                <div class="well well-sm">
                                    <form class="form-horizontal" method="post">
                                        <fieldset>

                                            <div class="form-group">
                                                <span class="col-md-1 col-md-offset-2 text-center"><i class="fa fa-address-card"></i></span>
                                                <div class="col-md-8">
                                                    <input id="documento" name="name" type="text" placeholder="Ingrese DNI" class="form-control">
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <span class="col-md-1 col-md-offset-2 text-center"><i class="fa fa-user bigicon"></i></span>
                                                <div class="col-md-8">
                                                    <input id="contrasena" name="name" type="text" placeholder="Ingrese Contraseña" class="form-control">
                                                </div>
                                            </div>

                                        </fieldset>
                                    </form>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Cerrar</button>
                    <button type="button" class="btn btn-danger" data-toggle="modal" data-target="#exampleModalRegistro">Ingresar</button>
                </div>
            </div>
        </div>
    </div>


    <div class="modal fade" id="exampleModalRegistro" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalRegistrarse">Registrarse</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">


                    <div class="container">
                        <div class="row">
                            <div class="col-md-12">
                                <div class="well well-sm">
                                    <form class="form-horizontal" method="post">
                                        <fieldset>


                                            <div class="form-group">
                                                <span class="col-md-1 col-md-offset-2 text-center"><i class="fa fa-address-card"></i></span>
                                                <div class="col-md-8">
                                                    <input id="dni" name="name" type="text" placeholder="Ingrese DNI" class="form-control">
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <span class="col-md-1 col-md-offset-2 text-center"><i class="fa fa-user bigicon"></i></span>
                                                <div class="col-md-8">
                                                    <input id="fname" name="name" type="text" placeholder="Ingrese Nombres" class="form-control">
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <span class="col-md-1 col-md-offset-2 text-center"><i class="fa fa-user bigicon"></i></span>
                                                <div class="col-md-8">
                                                    <input id="lname" name="name" type="text" placeholder="Ingrese Apellidos" class="form-control">
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <span class="col-md-1 col-md-offset-2 text-center"><i class="fa fa-home"></i></span>
                                                <div class="col-md-8">
                                                    <input id="direccion" name="name" type="text" placeholder="Ingrese Direccion" class="form-control">
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <span class="col-md-1 col-md-offset-2 text-center"><i class="fa fa-genderless"></i></span>
                                                <div class="col-md-8">
                                                    <input id="sexo" name="name" type="text" placeholder="Ingrese sexo" class="form-control">
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <span class="col-md-1 col-md-offset-2 text-center"><i class="fa fa-envelope-o bigicon"></i></span>
                                                <div class="col-md-8">
                                                    <input id="email" name="email" type="text" placeholder="Email Address" class="form-control">
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <span class="col-md-1 col-md-offset-2 text-center"><i class="fa fa-bars"></i></span>
                                                <div class="col-md-8">
                                                    <input id="estadocivil" name="name" type="text" placeholder="Ingrese estado civil" class="form-control">
                                                </div>
                                            </div>

                                            <div class="form-group">
                                                <span class="col-md-1 col-md-offset-2 text-center"><i class="fa fa-phone-square bigicon"></i></span>
                                                <div class="col-md-8">
                                                    <input id="phone" name="phone" type="text" placeholder="Ingrese telefono" class="form-control">
                                                </div>
                                            </div>

                                            <div class="form-group">
                                                <span class="col-md-1 col-md-offset-2 text-center"><i class="fa fa-pencil-square-o bigicon"></i></span>
                                                <div class="col-md-8">
                                                    <input id="correo" name="name" type="text" placeholder="Ingrese Correo electronico" class="form-control">
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <span class="col-md-1 col-md-offset-2 text-center"><i class="fa fa-check"></i></span>
                                                <div class="col-md-8">
                                                    <input id="nacionalidad" name="name" type="text" placeholder="Ingrese Nacionalidad" class="form-control">
                                                </div>
                                            </div>

                                            <div class="form-group">
                                                <span class="col-md-1 col-md-offset-2 text-center"><i class="fa fa-check-circle"></i></span>
                                                <div class="col-md-8">
                                                    <input id="departamento" name="name" type="text" placeholder="Ingrese Departamento" class="form-control">
                                                </div>
                                            </div>

                                        </fieldset>
                                    </form>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Cerrar</button>
                    <a class="btn btn-danger" href="#" role="button">Registrarse</a>
                </div>
            </div>
        </div>
    </div>


    <div class="modal fade" id="administrador" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="administradores">Login</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <div class="container">
                        <div class="row">
                            <div class="col-md-12">
                                <div class="well well-sm">
                                    <form class="form-horizontal" method="post">
                                        <fieldset>

                                            <div class="form-group">
                                                <span class="col-md-1 col-md-offset-2 text-center"><i class="fa fa-address-card"></i></span>
                                                <div class="col-md-8">
                                                    <input id="usuario" name="name" type="text" placeholder="Ingrese su usuario" class="form-control">
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <span class="col-md-1 col-md-offset-2 text-center"><i class="fa fa-user bigicon"></i></span>
                                                <div class="col-md-8">
                                                    <input id="password" name="name" type="text" placeholder="Ingrese Contraseña" class="form-control">
                                                </div>
                                            </div>

                                        </fieldset>
                                    </form>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Cerrar</button>
                    <a class="btn btn-danger" role="button">@Html.ActionLink("Ingresar", "Administrador", "Administrador")</a>
                </div>
            </div>
        </div>
    </div>


   
    @Scripts.Render("~/bundles/jquery")

</body>
</html>
